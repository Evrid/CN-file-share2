@model IEnumerable<StudentFileShare6.Models.UserSavedInfo>

<h1>Your Saved Documents</h1>

<table class="table">
    <thead>
        <tr>
            <th>Course Name</th>
            <th>Document Name</th>
            <th>School Name</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.CourseName</td>
                <td>@item.DocumentName</td>
                <td>@item.SchoolName</td>
            </tr>
        }
    </tbody>
</table>
